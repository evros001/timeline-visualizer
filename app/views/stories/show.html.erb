<div class="nav-text" id="text"><br><br><br><br><br><br>
  <%= @user.name.capitalize %>'s Story:<br> <%= @story.name %>
  <% if user_signed_in? && current_user.id == @user.id %>
    <%= link_to 'Edit', edit_user_story_path(@user.id, @story.id) %> |
    <%= link_to "Delete", user_story_path(@user.id, @story.id), method: :delete, data: { confirm: 'Are you sure you want to delete this?' } %>
  <% end %>
</div>
<div id="map-container">
  <div id="map-canvas"></div>
</div>
